<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
    "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
    "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions> 
  <!-- Allow XClone naming -->
  <suppress checks="AbbreviationAsWordInName" files=".*XClone.*\.java"/>
  
  <!-- Suppress Javadoc in test classes -->
  <suppress checks="MissingJavadoc" files=".*Test\.java"/>
</suppressions>
